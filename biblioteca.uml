@startuml
left to right direction
actor "Socio" as s
actor "Bibliotecario" as b
rectangle Biblioteca {
  usecase "Identificarse" as UC1
  usecase "Solicitar préstamo" as UC2
  usecase "Devolver libros" as UC3
  usecase "Consultar catálogo" as UC4
  usecase "Registrar libros" as UC5
  usecase "Gestionar socios" as UC6
  usecase "Añadir datos" as UC7
  usecase "Actualizar datos" as UC8
}
s --> UC1
b --> UC1
UC3 .> UC1 : include
UC2 .> UC1 : include
UC4 .> UC1 : include
UC5 .> UC1 : include
UC6 .> UC1 : include
UC7 .> UC6 : extends
UC8 .> UC6 : extends
@enduml
